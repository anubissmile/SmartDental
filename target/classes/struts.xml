<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<!-- version 1.2 -->
<struts>
    <constant name="struts.devMode" value="true" />

    <!-- RESTFUL STRUTS WILDCARD -->
    <!-- FOR FURTHER INFO ABOUT THIS -->
    <!-- https://struts.apache.org/docs/wildcard-mappings.html -->
    <constant name="struts.enable.SlashesInActionNames" value="true"/>
    <constant name="struts.mapper.alwaysSelectFullNamespace" value="false"/>
    <constant name="struts.patternMatcher" value="regex" />
    <!-- RESTFUL STRUTS WILDCARD -->

    <package name="SmartDental" extends="struts-default">

        <global-results>
            <result name="login">/login.jsp</result>
            <result name="noaccess">/noaccess.jsp</result>
            <result name="getCustomer">/index.jsp</result>
        </global-results>

        <action name="test/{userID}" class="com.smict.person.action.BranchAction" method="testWildcard">
            <result name="success" type="redirectAction">home</result>
        </action>

        <action name="logout" class="com.smict.auth.AuthAction" method="logOut">
            <result name="success" type="redirectAction">authenticate</result>
            <result name="error" type="redirectAction">home</result>
        </action>

        <action name="authenticate" class="com.smict.auth.AuthFormController" method="execute">
            <result name="success">/login.jsp</result>
        </action>
        <action name="home" class="com.smict.page.action.HomeAction" method="execute">
            <result name="success">/index.jsp</result>
        </action>

        <action name="attempt" class="com.smict.auth.AuthAction" method="authAttempt">
            <result name="success" type="redirectAction">home</result>
            <result name="input">/login.jsp</result>
            <result name="already" type="redirectAction">home</result>
        </action>

        <!-- ADD NEW TREATMENT ROOM -->
        <action name="addTreatmentRoom" class="com.smict.person.action.TreatmentRoomAction" method="addNewRoom">
            <result name="success" type="redirectAction">branchView?chkDetail=detail</result>
        </action>

        <action name="branchView" class="com.smict.person.action.BranchAction" method="execute">
			<param name="location">view</param>
            <result name="success">/branch.jsp</result>
            <result name="detail">/branch-detail.jsp</result>
        </action>
        <!-- ADD NEW TREATMENT ROOM -->

        <!-- DENTIST & ASSISTANCE SCHEDULES -->
        <action name="dentist-schedule" class="com.smict.schedule.action.ScheduleAction" 
                method="dentistScheduleForm">
            <result name="success">/dentist-schedule.jsp</result>
        </action>
        <action name="add-dentist-schedule" class="com.smict.schedule.action.ScheduleAction" 
                method="addDentistSchedule">
            <result name="success" type="redirectAction">dentist-schedule</result>
            <result name="input">/dentist-schedule.jsp</result>
        </action>
        <!-- DENTIST & ASSISTANCE SCHEDULES -->

        <!-- CHUNKING BRANCH -->
        <action name="patientBranch" class="com.smict.person.action.PatientBranchAction" method="execute">
          <result name="success">/addPatientBranch.jsp</result>
        </action>
        <!-- CHUNKING BRANCH -->

        <!-- ADD BRANCH HN (TYPE REDIRECT ACTION) -->
        <action name="addBranchHN" class="com.smict.person.action.AddBranchHNAction" method="execute">
            <result name="success" type="redirectAction">patientBranch</result>
        </action>
        <!-- ADD BRANCH HN (TYPE REDIRECT ACTION) -->

        <!--  ++++++   Top menu    ++++++   -->
        <action name="topPatient" class="com.smict.top.action.TopAction" method="execute">
            <result name="success">/patient.jsp</result>
        </action>
        <!--  ++++++   Top menu    ++++++   -->
        <!-- MASTER -->
        <!--  ++++++   Product Brand Master    ++++++   -->
        <action name="pbmBegin" class="com.smict.product.action.ProductBrandAction" method="begin">
            <result name="success">/master-productbrand.jsp</result>
        </action>


        <action name="productBrandMaster" class="com.smict.product.action.ProductBrandAction" method="execute">
            <result name="success">/master-productbrand.jsp</result>
        </action>
        <!--  ++++++   Product Brand Master    ++++++   -->
        <!--  ++++++   Product Group Master    ++++++   -->
        <action name="pgmBegin" class="com.smict.product.action.ProductGroupAction" method="begin">
            <result name="success">/master-productgroup.jsp</result>
        </action>
        <action name="productGroupMaster" class="com.smict.product.action.ProductGroupAction" method="execute">
            <result name="success">/master-productgroup.jsp</result>
        </action>
        <!--  ++++++   Product Group Master    ++++++   -->
        <!--  ++++++   Product Unit Master   ++++++   -->
        <action name="pumBegin" class="com.smict.product.action.ProductUnitAction" method="begin">
            <result name="success">/master-productunit.jsp</result>
        </action>
        <action name="productUnitMaster" class="com.smict.product.action.ProductUnitAction" method="execute">
            <result name="success">/master-productunit.jsp</result>
        </action>
        <!--  ++++++   Product Unit Master   ++++++   -->
        <!--  ++++++   Product Type Master   ++++++   -->
        <action name="ptmBegin" class="com.smict.product.action.ProductTypeAction" method="begin">
            <result name="success">/master-producttype.jsp</result>
        </action>
        <action name="productTypeMaster" class="com.smict.product.action.ProductTypeAction" method="execute">
            <result name="success">/master-producttype.jsp</result>
        </action>
        <!--  ++++++   Product Type Master   ++++++   -->
        <!--  ++++++   Lab Master    ++++++   -->
        <action name="labBegin" class="com.smict.product.action.LabAction" method="begin">
            <result name="success">/backend-lab.jsp</result>
        </action>
        <action name="labMaster" class="com.smict.product.action.LabAction" method="execute">
            <result name="success">/backend-lab.jsp</result>
        </action>
        <!--  ++++++   Lab Master    ++++++   -->
        <!--  ++++++   Lab Branch    ++++++   -->
        <action name="labBranchBegin" class="com.smict.product.action.LabBranchAction" method="begin">
            <result name="success">/backend-lab-branch.jsp</result>
        </action>
        <action name="labBranchMaster" class="com.smict.product.action.LabBranchAction" method="execute">
            <result name="success">/backend-lab-branch.jsp</result>
        </action>
        <!--  ++++++   Lab Branch    ++++++   -->
        <!--  ++++++   Service Master    ++++++   -->
        <action name="serviceBegin" class="com.smict.product.action.ServiceAction" method="begin">
            <result name="success">/backend-lab-service.jsp</result>
        </action>
        <action name="serviceMaster" class="com.smict.product.action.ServiceAction" method="execute">
            <result name="success">/backend-lab-service.jsp</result>
        </action>
        <!--  ++++++   Service Master    ++++++   -->
        <!--  ++++++   Lab Master    ++++++   -->
        <action name="labModeBegin" class="com.smict.product.action.LabModeAction" method="begin">
            <result name="success">/backend-labmode.jsp</result>
        </action>
        <action name="labModeMaster" class="com.smict.product.action.LabModeAction" method="execute">
            <result name="success">/backend-labmode.jsp</result>
        </action>
        <!--  ++++++   Service Master    ++++++   -->
        <!--  ++++++   LabService Master   ++++++   -->
        <action name="labserviceBegin" class="com.smict.product.action.LabServiceAction" method="begin">
            <result name="success">/backend-labservice.jsp</result>
        </action>
        <action name="labserviceMaster" class="com.smict.product.action.LabServiceAction" method="execute">
            <result name="success">/backend-labservice.jsp</result>
        </action>
        <!--  ++++++   LabService Master   ++++++   -->
        <!--  ++++++   Product Brand   ++++++   -->
        <action name="brandBegin" class="com.smict.person.action.BrandAction" method="begin">
            <result name="success">/master-brand.jsp</result>
        </action>
        <action name="brandMaster" class="com.smict.person.action.BrandAction" method="execute">
            <result name="success">/master-brand.jsp</result>
        </action>
        <!--  ++++++   Product Brand   ++++++   -->
        <!--  ++++++   Product Branch    ++++++   -->
        <action name="branchBegin" class="com.smict.person.action.BranchAction" method="begin">
            <result name="success">/branch.jsp</result>
        </action>
        <action name="branchM" class="com.smict.person.action.BranchAction" method="execute">
            <!-- <result name="success">/branch.jsp</result> -->
            <result name="success" type="redirectAction">branchBegin</result>
            <result name="detail">/branch-detail.jsp</result>
        </action>

        <!-- Treatment room delete & update -->
        <action name="delTr-{room_id}-{branch_code}" class="com.smict.person.action.TreatmentRoomAction" method="deleteTreatmentRoom">
            <result name="none">/branch-detail.jsp</result>
        </action>
        <action name="editTr" class="com.smict.person.action.TreatmentRoomAction" method="editTreatmentRoom">
            <result name="none">/branch-detail.jsp</result>
        </action>
        <!-- Treatment room delete & update -->

        <action name="branchM-{branch_code}" class="com.smict.person.action.BranchAction" method="execute">
            <!-- <result name="success">/branch.jsp</result> -->
            <result name="success" type="redirectAction">branchBegin</result>
			<result name="detail">/branch-detail.jsp</result>
        </action>
        
        <action name="branchEdit" class="com.smict.person.action.BranchAction" method="branchEdit">
        	<result name="input">/branch-detail.jsp</result>
        </action>
        <!-- ++++++ Product Branch ++++++ -->
        <!-- MASTER -->
        <!-- ++++++ Setting Education ++++++ -->
        <action name="SettingEducationVOBegin" class="com.smict.person.action.EducationAction" method="education_vocabulary_begin">
            <result name="success">/patient-education-vo.jsp</result>
        </action>
        <action name="SettingEducationVOExcute" class="com.smict.person.action.EducationAction" method="education_vocabulary_excute">
            <result name="success">/patient-education-vo.jsp</result>
        </action>
        <action name="SettingEducationBegin" class="com.smict.person.action.EducationAction" method="education_begin">
            <result name="success">/patient-education.jsp</result>
        </action>
        <action name="SettingEducationExcute" class="com.smict.person.action.EducationAction" method="education_excute">
            <result name="success">/patient-education.jsp</result>
        </action>
        <!--        ++++++  Setting Education    ++++++       -->
        <!--              Setting Person  PreName           -->
        <action name="SettingPersonPreName" class="com.smict.person.action.PreNameAction" method="begin">
            <result name="success">/patient-prename.jsp</result>
        </action>
        <action name="SettingPersonPreNameExcute" class="com.smict.person.action.PreNameAction" method="excute">
            <result name="success">/patient-prename.jsp</result>
        </action>
        <!--              Setting Person  PreName           -->
        <!--              Setting Person  TelType           -->
        <action name="SettingPersonTelType" class="com.smict.person.action.TypeTelAction" method="begin">
            <result name="success">/patient-typeTel.jsp</result>
        </action>
        <action name="SettingPersonTelTypeExcute" class="com.smict.person.action.TypeTelAction" method="excute">
            <result name="success">/patient-typeTel.jsp</result>
        </action>
        <!--              Setting Person  TelType           -->
        <!--              Setting Person  Address Type            -->
        <action name="SettingPersonAddrType" class="com.smict.person.action.AddressTypeAction" method="begin">
            <result name="success">/patient-addrType.jsp</result>
        </action>
        <action name="SettingPersonAddrTypeExcute" class="com.smict.person.action.AddressTypeAction" method="excute">
            <result name="success">/patient-addrType.jsp</result>
        </action>
        <!--              Setting Person  Address Type            -->
        <!--              Setting Person  married Status            -->
        <action name="SettingPersonMarried" class="com.smict.person.action.MarriedStatusAction" method="begin">
            <result name="success">/patient-marriedStatus.jsp</result>
        </action>
        <action name="SettingPersonMarriedExcute" class="com.smict.person.action.MarriedStatusAction" method="excute">
            <result name="success">/patient-marriedStatus.jsp</result>
        </action>
        <!--              Setting Person  married Status            -->
        <!--              Setting Person  register recommended            -->
        <action name="SettingPersonRecommended" class="com.smict.person.action.ReccommendedAction" method="begin">
            <result name="success">/patient-recommended.jsp</result>
        </action>
        <action name="SettingPersonRecommendedExcute" class="com.smict.person.action.ReccommendedAction" method="excute">
            <result name="success">/patient-recommended.jsp</result>
        </action>
        <!--              Setting Person  register recommended          -->
        <!--              Setting Person  doctor add          -->
        <action name="Doctor" class="com.smict.person.action.DoctorAction" method="begin">
            <result name="success">/doctor.jsp</result>
        </action>
        <action name="DoctorAddExcute" class="com.smict.person.action.DoctorAction" method="excute">
            <result name="success">/doctor-add.jsp</result>
        </action>
        <action name="GetDoctor" class="com.smict.person.action.DoctorAction" method="getDoctorDetail">
            <result name="success">/doctor-edit.jsp</result>
            <result name="error">/doctor.jsp</result>
        </action>
        <action name="UpdateDoctor" class="com.smict.person.action.DoctorAction" method="UpdateDoctor">
            <result name="success" type="redirectAction">GetDoctor</result>
        </action>
        <action name="DoctorType" class="com.smict.person.action.DoctorAction" method="Doctype_begin">
            <result name="success">/doctor_type.jsp</result>
        </action>
        <action name="DoctorTypeExcute" class="com.smict.person.action.DoctorAction" method="Doctype_excute">
            <result name="success">/doctor_type.jsp</result>
        </action>
        <action name="DoctorTimeBegin" class="com.smict.person.action.DoctorAction" method="DocTime_begin">
            <result name="success">/doctor-time.jsp</result>
        </action>
        <action name="DoctorTimeExcute" class="com.smict.person.action.DoctorAction" method="DocTime_excute">
            <result name="success">/doctor-time.jsp</result>
        </action>
        <!--              Setting Person  doctor add            -->
        <!--              Lab Process         -->
        <action name="sendLabBegin" class="com.smict.product.action.SendLabAction" method="begin">
            <result name="success">/lab.jsp</result>
        </action>
        <action name="sendLab" class="com.smict.product.action.SendLabAction" method="execute">
            <result name="success">/lab.jsp</result>
        </action>
        <action name="sendLabWindow" class="com.smict.product.action.SendLabAction" method="window">
            <result name="success">/lab-window.jsp</result>
        </action>
        <action name="patientMaster" class="com.smict.person.action.PatientAction" method="window">
            <result name="success">/window-patient-master.jsp</result>
        </action>
        <!--              Lab Process           -->
        <!--        ++++++   Treatment Master    ++++++       -->
        <action name="treatmentBegin" class="com.smict.treatment.action.TreatmentMasterAction" method="begin">
            <result name="success">/treatment_code_continous.jsp</result>
        </action>
        <action name="treatmentMaster" class="com.smict.treatment.action.TreatmentMasterAction" method="execute">
            <result name="success">/treatment_code_continous.jsp</result>
            <result name="error">/treatment_code_continous.jsp</result>
        </action>
        <action name="treatmentGroupBegin" class="com.smict.treatment.action.TreatmentGroupAction" method="begin">
            <result name="success">/treatment-sub-group.jsp</result>
        </action>
        <action name="treatmentGroupExcute" class="com.smict.treatment.action.TreatmentGroupAction" method="execute">
            <result name="success">/treatment-sub-group.jsp</result>
        </action>
        <!--        ++++++   Treatment Master    ++++++       -->
        <!--        ++++++   Treatment Waiting   ++++++       -->
        <action name="treatmentWaitingBegin" class="com.smict.treatment.action.TreatmentAction" method="BeginTreatment">
            <result name="success">/treatment_queue.jsp</result>
        </action>
        <action name="treatmentStatus" class="com.smict.treatment.action.TreatmentAction" method="waiting">
            <result name="success">/treatment.jsp</result>
        </action>
        <action name="treatmentSuccess" class="com.smict.treatment.action.TreatmentAction" method="treatmentsuccess">
            <result name="success">/treatment.jsp</result>
        </action>
        <action name="treatmentHistory" class="com.smict.treatment.action.TreatmentAction" method="treatmentHistory">
            <result name="success">/treatment.jsp</result>
        </action>
        <action name="treatmentContinue" class="com.smict.treatment.action.TreatmentAction" method="treatmentContinue">
            <result name="success">/treatment.jsp</result>
        </action>
        <action name="treatmentDrug" class="com.smict.treatment.action.TreatmentAction" method="treatmentDrug">
            <result name="success">/treatment.jsp</result>
        </action>
        <action name="treatmentProduct" class="com.smict.treatment.action.TreatmentAction" method="treatmentProduct">
            <result name="success">/treatment.jsp</result>
        </action>
        <action name="treatmentDeleteTran" class="com.smict.treatment.action.TreatmentAction" method="treatmentDelete">
            <result name="success">/treatment.jsp</result>
        </action>
        <action name="treatmentAlert" class="com.smict.treatment.action.TreatmentAction" method="alert">
            <result name="success">/treatment.jsp</result>
        </action>
        <action name="treatmentPrint" class="com.smict.treatment.action.TreatmentAction" method="print">
            <result name="success">/report/report-medicine.jsp</result>
        </action>
        <!--        ++++++   Treatment Waiting   ++++++       -->

    </package>
    <include file="struts-person.xml"></include>
    <include file="struts-treatmentplan.xml"></include>
    <include file="struts-document.xml"></include>
