	<queryString>
		<![CDATA[SELECT concat(DATE_FORMAT(a.create_date, '%d/%m')
,'/',DATE_FORMAT(a.create_date, '%Y')+543) as txtdate
,DATE_FORMAT(a.create_date, '%k:%i:%s')as txttime
, a.create_date
,a.hn,concat(a.pat_pre_name,' ',a.pat_firstname_th,' ',a.pat_lastname_th)as pat_name
,concat(a.doctor_pre_name,' ',a.doctor_firstname_th,' ',a.doctor_lastname_th)as doctor_name
,concat(a.emp_pre_name,' ',a.emp_firstname_th,' ',a.emp_lastname_th)as emp_name
,b.qty, b.price, (CASE WHEN b.pay_sso > 0 THEN b.pay_sso ELSE b.pay_amount END) as amt
,IFNULL(CASE WHEN b.paylast_type = 't' THEN
(b.paid_amount-(CASE WHEN b.pay_sso > 0 THEN b.pay_sso ELSE b.pay_amount END))
ELSE 0 END, 0) as paidamt
,b.paid_amount
,CASE WHEN b.paylast_type = 't' THEN b.paid_amount
ELSE (CASE WHEN b.pay_sso > 0 THEN b.pay_sso ELSE b.pay_amount END) END as amt_split
,IFNULL(b.discount,0)as discount
,CASE
 WHEN b.product_type = 7 THEN treat.code
 WHEN b.product_type = 1 THEN med.product_id
 ELSE pro.product_id
END as code_text
,CASE
 WHEN b.product_type = 7 THEN treat.nameth
 WHEN b.product_type = 1 THEN med.product_name
 ELSE pro.product_name
END as name_text
,ifnull(c.price,0) as price_df
,b.paylast_type
,d.channel_id as channel_money,e.channel_id as channel_credit
,f.channel_id as channel_deposit,g.channel_id as channel_sso
,ifnull(d.amount,0) as cha_amt_money,ifnull(e.amount,0) as cha_amt_credit
,ifnull(f.amount,0) as cha_amt_deposit,ifnull(g.amount,0) as cha_amt_sso
FROM order_receipt_owe a
inner join order_line_receipt_owe b on(b.owe_id = a.id)
left join treatment_master treat on(treat.id = b.product_id and b.product_type = 7)
left join pro_product med on(med.product_id = b.product_id and b.product_type = 1)
left join pro_product pro on(pro.product_id = b.product_id and b.product_type = 2)
left join order_payment_type c on(c.receipt_id = a.id and c.treatment_id = b.product_id
and b.product_type = 7 and c.type = 'doc')
left join order_payment_channel_receipt d on(d.receipt_id = a.id and d.channel_id in (1))
left join order_payment_channel_receipt e on(e.receipt_id = a.id and e.channel_id in (2))
left join order_payment_channel_receipt f on(f.receipt_id = a.id and f.channel_id in (4))
left join order_payment_channel_receipt g on(g.receipt_id = a.id and g.channel_id in (7))
WHERE a.id = $P{prmid}
ORDER BY a.id,b.id]]>
	</queryString>
